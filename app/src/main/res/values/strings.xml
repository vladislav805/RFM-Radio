	<string name="pref_recording_name_title">Filename</string>
	<string name="pref_recording_name_value" translatable="false">FM-$h$i$s-$f</string>
	<string name="pref_recording_path_name_description" tools:ignore="TypographyDashes">Available variables:\n$y - year\n$m - month\n$d - day of month\n$h - hour\n$i - minute\n$s - second\n$f - frequency (e.g., 103.4 MHz = 1034)\n$r - random number 10000-99999\nExample:\nDirectory: FM/$y-$m-$d -> FM/2020-01-27\nFilename: Rec-$h_$i_$s_$f -> Rec_23_01_49_0906.wav</string>
	<string name="pref_recording_format_title">Recording format</string>
